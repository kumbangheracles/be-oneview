          "password": {
            "type": "string",
            "example": "1234"
          }
        },
        "xml": {
          "name": "LoginRequest"
        }
      },
      "ActivationRequest": {
        "type": "object",
        "properties": {
          "code": {
            "type": "string",
            "example": "abcdef"
          }
        },
        "xml": {
          "name": "ActivationRequest"
        }
      }
    }
